{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2020",
        "jsx": "react",
        "allowImportingTsExtensions": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "types": [
            "babel__generator"
        ],
        "typeRoots": [
            "node_modules/@types",
        ],
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ],
}
